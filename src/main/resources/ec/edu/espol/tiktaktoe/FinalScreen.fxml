<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="ec.edu.espol.tiktaktoe.FinalScreenController">
    <stylesheets>
        <URL value="@/styles/finalscreen.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="200.0" prefWidth="382.0">
         <children>
            <Label fx:id="finalResult" text="Result" />
            <HBox prefHeight="100.0" prefWidth="298.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#playAgain" text="Play Again" />
                  <Button mnemonicParsing="false" onAction="#initialScreen" text="Initial Screen" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#exitBtn" text="Exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
