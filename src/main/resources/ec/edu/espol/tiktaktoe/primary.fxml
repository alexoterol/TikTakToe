<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" styleClass="mainFxmlClass" stylesheets="@../../../../styles/primaryInitial.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.tiktaktoe.PrimaryController">
   <children>
      <Label styleClass="labelTxt" stylesheets="@../../../../styles/primaryInitial.css" text="Tik Tak Toe" />
      <HBox alignment="CENTER" prefHeight="70.0" prefWidth="402.0" styleClass="hbox" stylesheets="@../../../../styles/primaryInitial.css">
         <children>
            <Button id="Button" fx:id="primaryButton" onAction="#switchToSecondary" stylesheets="@../../../../styles/primaryInitial.css" text="Start">
               <styleClass>
                  <String fx:value="Button" />
                  <String fx:value="Start" />
               </styleClass>
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin></Button>
            <Button id="Button" fx:id="exitButton" mnemonicParsing="false" onAction="#exitProgram" stylesheets="@../../../../styles/primaryInitial.css" text="Exit">
               <styleClass>
                  <String fx:value="Button" />
                  <String fx:value="Exit" />
               </styleClass></Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
